{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "server=DB_HOST;user=DB_USERNAME;password=DB_PASSWORD;database=priceapp",
    "Proxy": "server=DB_HOST;user=DB_USERNAME;password=DB_PASSWORD;database=priceapp_proxy"
  },
  "JWTSetting": {
    "SecretKey": "JWT_SECRET_KEY",
    "Issuer": "priceapp-server",
    "Audience": "priceapp-client",
    "Lifetime": 129600
  },
  "Cors": {
    "Url": "http://priceapp.co",
    "UrlSSL": "https://priceapp.co",
    "UrlLocal": "http://localhost:4200"
  },
  "Mail": {
    "From": "info@priceapp.co",
    "Login": "EMAIL_LOGIN_EMAIL",
    "Password": "EMAIL_PASS",
    "Host": "EMAIL_SMTP_SERVER",
    "Port": 587,
    "Name": "priceapp"
  },
  "Domain": {
    "Host": "https://priceapp.co",
    "Pretty": "priceapp.co",
    "Api": "https://api.priceapp.co",
    "Background": "https://background.api.priceapp.co",
    "ProxyApi": "https://proxy.api.priceapp.co"
  },
  "Threads": {
    "DefaultCount": 4,
    "UseSystem": true,
    "UseMultiThreading": false
  },
  "Proxy": {
    "MultiInstance": true,
    "IsProxy": false,
    "IsBackground": false
  }
}
