{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "server=DB_HOST;user=DB_USERNAME;password=DB_PASSWORD;database=priceapp",
    "Proxy": "server=DB_HOST;user=DB_USERNAME;password=DB_PASSWORD;database=priceapp_proxy"
  },
  "JWTSetting": {
    "SecretKey": "JWT_SECRET_KEY",
    "Issuer": "priceapp-server",
    "Audience": "priceapp-client",
    "Lifetime": 129600
  },
  "Threads": {
    "DefaultCount": 4,
    "UseSystem": true,
    "UseMultiThreading": false
  },
  "Proxy": {
    "MultiInstance": true,
    "IsProxy": false,
    "IsBackground": true
  }
}
